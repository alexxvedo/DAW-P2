<web-app>
    <servlet>
        <servlet-name>ControladorCarrito</servlet-name>
        <servlet-class>minitienda2.ControladorCarrito</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ControladorCarrito</servlet-name>
        <url-pattern>/ControladorCarrito</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>verCarrito</servlet-name>
        <servlet-class>minitienda2.verCarrito</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>caja</servlet-name>
        <servlet-class>minitienda2.caja</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>agregarMusica</servlet-name>
